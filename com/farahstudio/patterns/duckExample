package com.farahstudio.patterns.duckExample;

import com.farahstudio.patterns.duckExample.behaviors.FlyBehavior;
import com.farahstudio.patterns.duckExample.behaviors.QuackBehavior;

public class Duck {
	
	FlyBehavior flyBehavior;
	QuackBehavior quackBehavior;
	
	
	public void performQuack () {
		
		quackBehavior.quack();
	}
	
	public void swim() {
		
	}
	
	public void display() {
		
	}
	
	public void performFly() {
		
		flyBehavior.fly();
	}
	
}
